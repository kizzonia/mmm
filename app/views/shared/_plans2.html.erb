                  <div class="featured-icon-box style5 text-left mb-20">
                      <div class="featured-icon"><!-- featured-icon-->
                          <div class="ttm-icon ttm-icon_element-color-white ttm-icon_element-size-lg">
                              <i class="flaticon flaticon-intelligent"></i>
                          </div>
                      </div><!-- featured-icon -->
                      <div class="featured-content"><!-- featured-content -->
                          <div class="featured-title">
                              <h5><%=  b.title %></h5><!-- featured-title -->
                          </div>
                          <div class="featured-desc"><!-- featured-description -->
                              <p> <%= b.sub_title %>.</p>
                          </div>
                          <%= link_to b, class: "ttm-btn ttm-btn-size-sm ttm-btn-color-white btn-inline ttm-icon-btn-right mt-15" do %>
                          Read More <i class="ti ti-angle-double-right"></i> <% end %>
                      </div><!-- featured-content END-->
                  </div><!-- featured-icon-box -->
              </div>
              <% end %>

          </div><!-- row end-->
      </div>
  </section>



