                      <div class="title-header">
                          <h5><%= h.plans_title %></h5>
                          <h2 class="title"><%= h.plans_sub_title %> <span>experience</span></h2>
                      </div>
                      <% end %>
                  </div><!-- section title end -->
              </div>
          </div>
          <!-- row end -->
          <!-- row -->
          <div class="row">
            <% @plans.take(3).each do |b| %>

              <div class="col-lg-4 col-md-4">
