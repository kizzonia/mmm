<p id="notice"><%= notice %></p>

<h1>Adminloans</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Status</th>
      <th>Verified</th>
      <th>User</th>
      <th>Title</th>
      <th>Loan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adminloans.each do |adminloan| %>
      <tr>
        <td><%= adminloan.body %></td>
        <td><%= adminloan.status %></td>
        <td><%= adminloan.verified %></td>
        <td><%= adminloan.user_id %></td>
        <td><%= adminloan.title %></td>
        <td><%= adminloan.loan_id %></td>
        <td><%= link_to 'Show', adminloan %></td>
        <td><%= link_to 'Edit', edit_adminloan_path(adminloan) %></td>
        <td><%= link_to 'Destroy', adminloan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Adminloan', new_adminloan_path %>
